    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 120,
    "importOrder": [
        "^[.]{2}",
        "^[.]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "overrides": [
